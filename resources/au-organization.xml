    </element>
    <element id="Organization.type">
      <path value="Organization.type" />
      <binding>
        <strength value="preferred" />
        <valueSet value="https://healthterminologies.gov.au/fhir/ValueSet/healthcare-organisation-role-type-1" />
      </binding>
