  <differential>
    <element id="Identifier">
      <path value="Identifier"/>
      <short value="Electronic Transfer of Prescription (ETP) Vendor Identifier"/>
      <definition value="Electronic Transfer of Prescription (ETP) vendor identifier assigned by an ETP vendor, e.g. ERx or Medisecure, to a prescription. This identifier is the assigned numeric value of the barcode on a prescription instance."/>
    </element>
    <element id="Identifier.type">
      <path value="Identifier.type"/>
      <min value="1"/>
      <binding>
        <strength value="required"/>
        <description value="Local Identifier Type"/>
        <valueSet value="http://hl7.org.au/fhir/ValueSet/au-hl7v2-0203"/>
      </binding>
    </element>
    <element id="Identifier.type.coding">
      <path value="Identifier.type.coding"/>
      <min value="1"/>
      <max value="1"/>
    </element>
    <element id="Identifier.type.coding.system">
      <path value="Identifier.type.coding.system"/>
      <min value="1"/>
      <fixedUri value="http://terminology.hl7.org.au/CodeSystem/v2-0203"/>
    </element>
    <element id="Identifier.type.coding.code">
      <path value="Identifier.type.coding.code"/>
      <min value="1"/>
      <fixedCode value="ETP"/>
    </element>
    <element id="Identifier.type.text">
      <path value="Identifier.type.text"/>
      <min value="1"/>
      <fixedString value="ETP Identifier"/>
    </element>
    <element id="Identifier.system">
      <path value="Identifier.system"/>
      <short value="ETP vendor identifier system URL"/>
      <definition value="URL assigned by the ETP (electronic transfer of prescription) vendor allocating the identifier."/>
      <min value="1"/>
    </element>
    <element id="Identifier.value">
      <path value="Identifier.value"/>
      <short value="ETP vendor identifier"/>
      <definition value="ETP Vendor allocated identifier for the request this dispense record is fulfilling."/>
      <min value="1"/>
    </element>
    <element id="Identifier.assigner">
      <path value="Identifier.assigner"/>
      <short value="ETP vendor"/>
      <definition value="ETP Vendor assigning this identifier"/>
      <min value="1"/>
    </element>
    <element id="Identifier.assigner.display">
      <path value="Identifier.assigner.display"/>
      <short value="ETP vendor name"/>
      <definition value="Name of the ETP vendor assigning this identifier."/>
      <min value="1"/>
    </element>
